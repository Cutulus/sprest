{
  "name": "gd-sprest",
  "version": "2.3.0",
  "description": "An easy way to develop against the SharePoint REST API.",
  "author": "Gunjan Datta <me@dattabase.com> (https://gunjandatta.github.io/sprest)",
  "license": "MIT",
  "main": "build/gd-sprest.js",
  "typings": "build/gd-sprest.d.ts",
  "keywords": [
    "sharepoint",
    "sharepoint 2013",
    "sharepoint online",
    "rest",
    "api",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gunjandatta/sprest.git"
  },
  "bugs": {
    "url": "https://github.com/gunjandatta/sprest/issues"
  },
  "homepage": "https://gunjandatta.github.io/sprest",
  "dependencies": {},
  "devDependencies": {
    "dts-bundle": "^0.7.3",
    "uglify-js": "^3.2.0",
    "webpack": "^3.9.1"
  },
  "scripts": {
    "build": "tsc && webpack",
    "build-lib": "dts-bundle --name gd-sprest --main build/gd-sprest.d.ts --out ../dist/gd-sprest.d.ts",
    "minify": "uglifyjs --compress --mangle --output ./dist/gd-sprest.min.js ./dist/gd-sprest.js",
    "package": "npm run build && npm run build-lib && npm run minify"
  }
}
